<div id="<%= dom_id customer %>">
  <p>
    <strong>Customerid:</strong>
    <%= customer.customerid %>
  </p>

  <p>
    <strong>Fname:</strong>
    <%= customer.fname %>
  </p>

  <p>
    <strong>Ifsc:</strong>
    <%= customer.ifsc %>
  </p>

  <p>
    <strong>Lname:</strong>
    <%= customer.lname %>
  </p>

  <p>
    <strong>Mobile:</strong>
    <%= customer.mobile %>
  </p>

  <p>
    <strong>Pancard:</strong>
    <%= customer.pancard %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= customer.email %>
  </p>

  <p>
    <strong>District:</strong>
    <%= customer.district %>
  </p>

  <p>
    <strong>State:</strong>
    <%= customer.state %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= customer.country %>
  </p>

  <%= link_to "Show this customer", "/customers/#{customer.customerid}" %>

</div>
